              </div>
            </div>

            <!-- <div class="rent">
                <h3>Rent</h3>
                <h4 class="rent_value">1200$</h4>
             </div> -->

            <div class="tenant">
              <h3>tenant</h3>
              <h4 class="tenant_value">Manan Jiwnani</h4>
            </div>
            <a href="" class="details">More Details</a>
          </div>
        </div>
        <h1 class="MAIN_text">Other Properties</h1>
      </div>
      <div class="feature_display display"></div>
      <div class="feature_display display"></div>
      <div class="feature_display display"></div>
